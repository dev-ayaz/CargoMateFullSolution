<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>CargoMate | Dashboard</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/datatables/datatables.min.css" rel="stylesheet" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <link href="~/AppLayout/global/plugins/select2/css/select2.css" rel="stylesheet" />
    <link href="~/AppLayout/global/plugins/select2/css/select2-bootstrap.min.css" rel="stylesheet" />
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <link href="~/AppLayout/global/plugins/bootstrap-daterangepicker/daterangepicker.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/morris/morris.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/fullcalendar/fullcalendar.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/global/plugins/jqvmap/jqvmap/jqvmap.css" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL STYLES -->
    <link href="~/AppLayout/global/css/components.min.css" rel="stylesheet" id="style_components" type="text/css" />
    <link href="~/AppLayout/global/css/plugins.min.css" rel="stylesheet" type="text/css" />
    <!-- END THEME GLOBAL STYLES -->
    <!-- BEGIN THEME LAYOUT STYLES -->
    <link href="~/AppLayout/layouts/layout4/css/layout.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AppLayout/layouts/layout4/css/themes/light.min.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="~/AppLayout/layouts/layout4/css/custom.min.css" rel="stylesheet" type="text/css" />
    <!-- END THEME LAYOUT STYLES -->
    <link href="~/AppLayout/global/plugins/bootstrap-toastr/toastr.css" rel="stylesheet" />
    <link href="~/AppLayout/global/plugins/sweetalert/sweetalert.css" rel="stylesheet" />
    <title>@ViewBag.Title - CargoMate Application</title>

</head>
<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo">
    @Html.Partial("_Header")
    <!-- BEGIN HEADER & CONTENT DIVIDER -->
    <div class="clearfix"> </div>
    <!-- END HEADER & CONTENT DIVIDER -->
    <!-- BEGIN CONTAINER -->
    <div class="page-container">

        @Html.Partial("_SideBar")
        <div class="page-content-wrapper">
            @RenderBody()
        </div>
        <!-- BEGIN FOOTER -->
        <div class="page-footer">
            <div class="page-footer-inner">
                2017 &copy; CargoMate
                <a href="#" title="CargoMate" target="_blank">SubscribeCargoMate news Letter</a>
            </div>
            <div class="scroll-to-top">
                <i class="icon-arrow-up"></i>
            </div>
        </div>
        <!-- END FOOTER -->
    </div>
    <!-- BEGIN CORE PLUGINS -->
    <script src="~/AppLayout/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.bootstrap.min.js"></script>

    <script src="~/AppLayout/global/plugins/js.cookie.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="~/AppLayout/global/plugins/moment.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/bootstrap-daterangepicker/daterangepicker.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/morris/morris.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/morris/raphael-min.js" type="text/javascript"></script>
    <script src="~/AppLayout/global/plugins/datatables/datatables.all.min.js"></script>
    <script src="~/AppLayout/global/plugins/select2/js/select2.full.js"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="~/AppLayout/global/scripts/app.min.js" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/AppLayout/pages/scripts/dashboard.min.js" type="text/javascript"></script>
    <script src="~/Scripts/CargoMateUtilities/ajax-interceptor.js"></script>
    <script src="~/Scripts/CargoMateUtilities/link-activator.js"></script>
    <script src="~/Scripts/CargoMateUtilities/AppAlerts.js"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="~/AppLayout/layouts/layout4/scripts/layout.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/layouts/layout4/scripts/demo.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/layouts/global/scripts/quick-sidebar.min.js" type="text/javascript"></script>
    <script src="~/AppLayout/layouts/global/scripts/quick-nav.js" type="text/javascript"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
    <script src="~/AppLayout/global/plugins/bootstrap-toastr/toastr.js"></script>
    <script src="~/AppLayout/global/plugins/sweetalert/sweetalert.min.js"></script>
    <!--Application Common Scripts-->
    <script src="~/Scripts/CargoMateUtilities/IAC.Utilities.js"></script>
<script>
    $(function() {
        $("select").select2({
            dropdownAutoWidth: true,
            width: false
        });
    });
   
</script>

    @RenderSection("Scripts", false)
</body>
</html>